{
    "name": "ai-search-agent",
    "version": "0.0.1",
    "private": true,
    "description": "AI-powered web search agent service",
    "scripts": {
        "dev": "powershell -ExecutionPolicy Bypass -File startup.ps1",
        "start": "powershell -ExecutionPolicy Bypass -File startup.ps1",
        "dev:batch": "startup.bat",
        "dev:python": "python -m uvicorn main:app --host 0.0.0.0 --port 8000 --reload",
        "setup": "python -m venv venv && call venv\\Scripts\\activate.bat && python -m pip install --upgrade pip && pip install -r requirements.txt",
        "install": "pip install -r requirements.txt",
        "test": "echo \"No tests specified\" && exit 0"
    },
    "keywords": [
        "ai",
        "search",
        "fastapi",
        "python"
    ],
    "author": "",
    "license": "MIT"
}