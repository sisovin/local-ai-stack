{
    "buildCommand": "pnpm --filter=web build",
    "installCommand": "pnpm install --filter=web --filter=@workspace/ui --filter=@workspace/eslint-config --filter=@workspace/typescript-config",
    "outputDirectory": "apps/web/.next",
    "framework": "nextjs",
    "functions": {
        "apps/web/app/api/**/*.ts": {
            "runtime": "nodejs20.x"
        }
    },
    "env": {
        "NEXT_PUBLIC_SUPABASE_URL": "https://placeholder.supabase.co",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY": "placeholder-key-for-build-time"
    }
}